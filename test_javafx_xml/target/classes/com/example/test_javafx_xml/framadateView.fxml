<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>


<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.test_javafx_xml.ControllerFramadate">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>


    <Label text="Saisissez un Url de sondage Framadate"/>
    <TextField fx:id="urlFramadateTextField" />
    <Button fx:id="confirmUrl" text="Valider" onMouseClicked="#addUrl"/>
    <Label fx:id="confirmationAdd" visible="false"/>

    <Button fx:id="backButton" text="Retour" onMouseClicked="#backAction"/>
</VBox>